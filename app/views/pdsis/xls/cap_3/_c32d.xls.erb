 <Worksheet ss:Name="quadro3.2D">
  <Table ss:ExpandedColumnCount="7" ss:ExpandedRowCount="400" x:FullColumns="1"
   x:FullRows="1" ss:StyleID="s536" ss:DefaultColumnWidth="60"
   ss:DefaultRowHeight="15">
   <Column ss:StyleID="s536" ss:AutoFitWidth="0" ss:Width="180.75"/>
   <Column ss:StyleID="s536" ss:AutoFitWidth="0" ss:Width="180.75"/>
   <Column ss:StyleID="s536" ss:AutoFitWidth="0" ss:Width="180.75"/>
   <Column ss:StyleID="s536" ss:Width="207"/>
   <Column ss:StyleID="s536" ss:AutoFitWidth="0" ss:Width="180.75"/>
   <Column ss:StyleID="s536" ss:AutoFitWidth="0" ss:Width="180.75"/>
   <Row ss:AutoFitHeight="0" ss:Height="50.25">
    <Cell ss:MergeAcross="5" ss:StyleID="m339197816"><Data ss:Type="String">QUADRO 3.2D - REDE DE SERVIÇOS DO SISTEMA ÚNICO DE SAÚDE PARA A COMPLEMENTARIEDADE DA ATENÇÃO BÁSICA, MÉDIA E  DE ALTA COMPLEXIDADE.  </Data></Cell>
   </Row>
   <Row ss:AutoFitHeight="0" ss:Height="80.25">
    <Cell ss:StyleID="s537"><Data ss:Type="String">POLO BASE</Data></Cell>
    <Cell ss:StyleID="s538"><Data ss:Type="String">POPULAÇÃO</Data></Cell>
    <Cell ss:StyleID="s538"><Data ss:Type="String">MUNICÍPIO</Data></Cell>
    <Cell ss:StyleID="s538"><Data ss:Type="String">MUNICÍPIOS REFERÊNCIA</Data></Cell>
    <Cell ss:StyleID="s538"><Data ss:Type="String">Estabelecimentos de saúde (Complementariedade da Atenção Básica)</Data></Cell>
    <Cell ss:StyleID="s538"><Data ss:Type="String">Estabelecimentos de saúde (Atenção Especializada)</Data></Cell>
    <Cell ss:StyleID="s539"/>
   </Row>
  <%= @dsei.base_polos_with_service_networks.order(:id).each do |base_polo| %>
    <%= base_polo.service_networks.each do |service_network| %>
      <%= service_network.service_network_cities.each do |city| %>
        <Row ss:AutoFitHeight="0" ss:Height="30">
         <Cell ss:StyleID="s580"><Data ss:Type="String"><%= base_polo.name %></Data></Cell>
         <Cell ss:StyleID="s581"><Data ss:Type="String"><%= base_polo.city_name %></Data></Cell>
         <Cell ss:StyleID="s584"><Data ss:Type="String"><%= base_polo.population %></Data></Cell>
         <Cell ss:StyleID="s584"><Data ss:Type="String"><%= city.city_name %></Data></Cell>
         <Cell ss:StyleID="s584"><Data ss:Type="String"><%= city.health_establishments.map { |item| item.name }.join(', ') %></Data></Cell>
         <Cell ss:StyleID="s584"><Data ss:Type="String"><%= city.health_specializeds.map { |item| item.name }.join(', ') %></Data></Cell>
        </Row>
      <% end %>
    <% end %>
  <% end %>
   <Row ss:AutoFitHeight="0" ss:Height="30">
    <Cell ss:StyleID="s590"/>
    <Cell ss:StyleID="s593"/>
    <Cell ss:StyleID="s593"/>
    <Cell ss:StyleID="s593"/>
    <Cell ss:StyleID="s593"/>
    <Cell ss:StyleID="s593"/>
   </Row>
  </Table>
